<!DOCTYPE html>
<html>
<body>

<%= form_for(@user, url: users_path)  do |f| %>
	
	<%= f.label :username%>
	<%= f.text_field :username %><br>

	<%= f.label :email %>
	<%= f.email_field :email %><br>

	<%= f.label :password %>
	<%= f.password_field :password %><br>

	<%= f.submit "Create" %>
	<% end %>













